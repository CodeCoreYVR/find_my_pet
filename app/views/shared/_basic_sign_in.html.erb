<%= button_to 'Sign In With Facebook', sign_in_with_facebook_path, class:"btn btn-default", method: :get %>
<p>or</p>
<%= button_to 'Sign In With Twitter', sign_in_with_twitter_path, class:"btn btn-default", method: :get %>

<%= form_tag sessions_path do %>
  <div class="form-group">
    <%= label_tag :email %>
    <%= text_field_tag :email, nil, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, nil, class: "form-control" %>
  </div>

  <p> Don't Have an account? </p>
  <a class="#" data-dismiss="modal" data-toggle="modal" data-target="#SignUp">Sign Up</a>

<div class="modal-footer">
  <%= submit_tag 'Sign In', class: "btn btn-default"%>
</div>
<% end %>
